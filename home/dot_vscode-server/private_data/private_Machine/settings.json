{
    "Lua.workspace.library": [
        "${env:VIMRUNTIME}/lua/",
        "${env:VIMRUNTIME}/lua/vim/lsp",
        "${env:HOME}/.local/share/nvim/lazy/ui/nvchad_types",
        "${env:HOME}/.local/share/nvim/lazy/lazy.nvim/lua/lazy"
    ],
    "bashIde.enableSourceErrorDiagnostics": true,
    "bashIde.includeAllWorkspaceSymbols": false,
    "files.watcherExclude": {
        "**/.cargo": true,
        "**/.dotnet": true,
        "**/.ghcup": true
    },
    "latex-utilities.countWord.docker.enabled": true,
    "mise.binPath": "/home/ajay/.local/bin/mise",
    "stylua.searchBinaryInPATH": true,
    "stylua.searchParentDirectories": true,
    "terminal.integrated.defaultProfile.linux": "bash",
    "typos.config": "${env:HOME}/typos.toml",
}
